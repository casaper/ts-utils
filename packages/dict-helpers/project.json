{
  "name": "dict-helpers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dict-helpers/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/dict-helpers",
        "tsConfig": "packages/dict-helpers/tsconfig.lib.json",
        "packageJson": "packages/dict-helpers/package.json",
        "main": "packages/dict-helpers/src/index.ts",
        "assets": ["packages/dict-helpers/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/dict-helpers/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/dict-helpers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:util"]
}
